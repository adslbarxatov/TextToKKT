<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RD_AAOW.SnPage">

    <ScrollView>
        <StackLayout>
            <StackLayout Orientation="Horizontal">
                <Label x:Name="KKTSNLabel" VerticalOptions="Center" />
                <Editor x:Name="KKTSN" />
            </StackLayout>
            <Label x:Name="KKTTypeLabel" />
            
            <Label x:Name="Filler1" />

            <StackLayout Orientation="Horizontal">
                <Label x:Name="FNSNLabel" VerticalOptions="Center" />
                <Editor x:Name="FNSN" />
            </StackLayout>
            <Label x:Name="FNTypeLabel" />

            <Label x:Name="Filler2" />

            <StackLayout Orientation="Horizontal">
                <Label x:Name="OFDINNLabel" VerticalOptions="Center" />
                <Editor x:Name="OFDINN" />
            </StackLayout>
            <Label x:Name="OFDNameLabel" />

            <Label x:Name="Filler3" />
        </StackLayout>
    </ScrollView>
</ContentPage>
