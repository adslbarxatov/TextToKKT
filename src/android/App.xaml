<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RD_AAOW.App">

	<Application.Resources>
		<ResourceDictionary>
			<Color x:Key="NPBack">#202020</Color>
			<Color x:Key="NPFore">#E0E0E0</Color>

			<Style TargetType="NavigationPage">
				<Setter Property="BarBackgroundColor" Value="{StaticResource NPBack}" />
				<Setter Property="BarTextColor" Value="{StaticResource NPFore}" />
			</Style>
		</ResourceDictionary>
	</Application.Resources>
	
</Application>
